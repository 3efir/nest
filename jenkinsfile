pipeline {
    agent { label 'development' }

    stages {
        stage('Install project') {
            steps {
                sh 'git status'
                sh 'npm ci'
            }
        }
        stage('Build project') {
            steps {
                sh 'npm run build'
            }
        }
        stage('Test project') {
            steps {
                sh 'npm run test'
            }
        }
    }
}
